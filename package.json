{
  "name": "old-joe",
  "version": "0.4.1",
  "description": "A test framework for Slack",
  "main": "./dist/src/main/main",
  "scripts": {
    "start": "rimraf ./dist && electron-forge start",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "make": "cross-env NODE_ENV=production electron-forge make",
    "package": "cross-env NODE_ENV=production electron-forge package",
    "publish": "cross-env NODE_ENV=production electron-forge publish",
    "parcel:build": "node ./tools/parcel-build.js",
    "parcel:watch": "node ./tools/parcel-watch.js"
  },
  "author": "Felix Rieseberg <felix@felixrieseberg.com>",
  "contributors": [
    "Carlos Valdez <carlosvr@utexas.edu>"
  ],
  "license": "ISC",
  "dependencies": {
    "@blueprintjs/core": "^3.19.0",
    "applescript": "^1.0.0",
    "chai": "^4.2.0",
    "cross-spawn": "^7.0.1",
    "debug": "^4.1.1",
    "electron-chromedriver": "electron/chromedriver#00514361859a4e607b93ba26d690c4b92daa7cc9",
    "electron-context-menu": "^0.15.0",
    "electron-create-menu": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "extract-zip": "^1.6.7",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15",
    "manage-wifi": "^1.0.1",
    "mobx": "^5.14.0",
    "mobx-react": "^6.1.3",
    "node-json2html": "1.2.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "robotjs": "^0.5.1",
    "semver": "^6.3.0",
    "shelljs": "0.8.3",
    "shortid": "^2.2.15",
    "tslib": "1.10.0",
    "webdriverio": "^5.14.4"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.45",
    "@electron-forge/maker-deb": "^6.0.0-beta.45",
    "@electron-forge/maker-rpm": "^6.0.0-beta.45",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.45",
    "@electron-forge/maker-zip": "^6.0.0-beta.45",
    "@electron-forge/publisher-github": "6.0.0-beta.45",
    "@types/chai": "^4.2.3",
    "@types/cross-spawn": "^6.0.0",
    "@types/extract-zip": "^1.6.2",
    "@types/fs-extra": "^8.0.0",
    "@types/node": "^12.7.12",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/semver": "^6.0.2",
    "@types/shelljs": "0.8.5",
    "@types/shortid": "0.0.29",
    "cross-env": "^6.0.3",
    "electron": "4.2.0",
    "less": "^3.10.3",
    "parcel-bundler": "^1.12.4",
    "prettier": "1.18.2",
    "rimraf": "^3.0.0",
    "typescript": "^3.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  }
}
